<div class="govuk-details__text govuk-!-margin-bottom-4">
  <p class="govuk-body"><%= t('solr_datasets.show.not_released') %></p>
  <p class="govuk-body">
    <% if contact_information_exists?(dataset) %>
      <%= t('solr_datasets.show.contact_the_publisher') %>
    <% else %>
