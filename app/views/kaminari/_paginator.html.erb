<%= link_to_prev_page @datasets, 'Prev' unless current_page.first?%>

<%= paginator.render do -%>
  <% each_page do |page| -%>
    <% if page.display_tag? -%>
      <%= page_tag page %>
    <% elsif !page.was_truncated? -%>
      <%= gap_tag %>
    <% end -%>
  <% end -%>
<% end -%>

<% unless current_page.out_of_range? %>
  <%= link_to_next_page @datasets, 'Next' unless current_page.last? %>
<% end %>
